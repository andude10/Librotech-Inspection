<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Brushes -->
    <SolidColorBrush x:Key="PrimaryBrush" Color="#E84807" />
    <SolidColorBrush x:Key="PrimaryBrushHighlight" Color="#FF5D08" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="#14699C" />
    <SolidColorBrush x:Key="SecondaryBrushHighlight" Color="#0E9FB3" />
    <SolidColorBrush x:Key="PrimaryForeground" Color="Black" />
    <SolidColorBrush x:Key="HighlightBrush" Opacity="0.8" Color="#1E33E8" />
    <SolidColorBrush x:Key="LightBackgroundBrush" Color="White" />

    <Thickness x:Key="DefBorderThickness">4</Thickness>

    <CornerRadius x:Key="DefCornerRadius">18</CornerRadius>

</ResourceDictionary>